-- noinspection SqlNoDataSourceInspectionForFile

create table player (
    uuid char(36),
    username varchar (32),
    primary key (uuid)
);
create table log (
    uuid     char(36),
    log_time char (19),
    log_type char (6),
    x        int,
    y        int,
    z        int,
    primary key (uuid, log_time, log_type),
    foreign key (uuid) references player
);
